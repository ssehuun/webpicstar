<script>//url에서 게임id 가져옴.
  getParameter = function(name){
      search=location.search;
      if(!search){
          //파라미터가 하나도 없을때
          document.write("에러 출력 텍스트");
          return false;
      }
   
      search=search.split("?");
      data=search[1].split("=");
      if(search[1].indexOf(name)==(-1) || data[0]!=name){
          //해당하는 파라미터가 없을때.
          return "";
          return;
      }
      if(search[1].indexOf("&")==(-1)){
          //한개의 파라미터일때.
          data=search[1].split("=");
          return data[1];
      }else{
      //여러개의 파라미터 일때.
      data=search[1].split("&"); //엠퍼센트로 자름.
      for(i=0;i<=data.length-1;i++){
          l_data=data[i].split("=");
          if(l_data[0]==name){
              return l_data[1];
              break;
          }else continue;
          }
      }
  }

var gameId = getParameter("gameId");

  $.ajax({//해외픽 불러오기
    type: 'get',
    data: {gameId: gameId},
    url: '/web/predictInfo/',
    dataType: 'html',
    success:function(data){
      $('#foreignPick').html(data);
    }   
  });
  $.ajax({//리그정보 불러오기
    type: 'get',
    data: {gameId: gameId},
    url: '/web/leagueInfo/',
    dataType: 'html',
    success:function(data){
      $('#leagueInfo').html(data);
    }   
  });
  $.ajax({//팀정보 불러오기
    type: 'get',
    data: {gameId: gameId},
    url: '/web/matchInfo/',
    dataType: 'html',
    success:function(data){
      $('#matchInfo').html(data);
    }   
  });
  $.ajax({//의견 불러오기
    type: 'get',
    data: {gameId: gameId},
    url: '/gameCommentList/',
    dataType: 'html',
    success:function(data){
      $('#opinion').html(data);
    }   
  });    
