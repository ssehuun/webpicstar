<script>//로그인
  $('#first').click(function(){
    var username = $('#username').val();
    var password = $('#password').val();
    //var nickname = $('#nickname').val();
    var params0 = {username:username, password:password};
    
    $.ajax({
      type: 'POST',
      data: params0,
      url: '/sgUp/',
      dataType: 'html',
      success: function(){              
      }
    })
  })
</script>




